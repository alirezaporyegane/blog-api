{
  "builds": [
    {
      "src": "./dist/app.js",
      "use": "@now/node"
    },
    {
      "src": "./dist/public",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "./dist/app.js"
    }
  ],
  "buildCommand": "npm run build",
  "devCommand": "npm run start"
}